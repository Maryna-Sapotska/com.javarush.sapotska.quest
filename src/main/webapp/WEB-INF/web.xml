<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>jsp</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>/index.jsp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jspGame</servlet-name>
        <jsp-file>/game.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>jspGame</servlet-name>
        <url-pattern>/game.jsp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jspEnd</servlet-name>
        <jsp-file>/end.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>jspEnd</servlet-name>
        <url-pattern>/end.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GameServlet</servlet-name>
        <servlet-class>org.example.comjavarushsapotskaquest.controller.GameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GameServlet</servlet-name>
        <url-pattern>/game</url-pattern>
    </servlet-mapping>
</web-app>